    <servlet-class>tools.vcode.servlet.VerifyCodeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BlogServlet</servlet-name>
    <servlet-class>blog.web.BlogServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TagServlet</servlet-name>
    <servlet-class>tag.web.TagServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MBlogServlet</servlet-name>
    <servlet-class>blog.web.MBlogServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MTagServlet</servlet-name>
    <servlet-class>tag.web.MTagServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TagsmapServlet</servlet-name>
    <servlet-class>tagsmap.web.TagsmapServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BaseServlet</servlet-name>
    <url-pattern>/BaseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VerifyCodeServlet</servlet-name>
    <url-pattern>/VerifyCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BlogServlet</servlet-name>
    <url-pattern>/BlogServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TagServlet</servlet-name>
    <url-pattern>/TagServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MBlogServlet</servlet-name>
    <url-pattern>/MBlogServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MTagServlet</servlet-name>
    <url-pattern>/MTagServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TagsmapServlet</servlet-name>
    <url-pattern>/TagsmapServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>MUserServlet</display-name>
    <servlet-name>MUserServlet</servlet-name>
    <servlet-class>user.web.MUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MUserServlet</servlet-name>
    <url-pattern>/MUserServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>LoginFilter</display-name>
    <filter-name>LoginFilter</filter-name>
    <filter-class>web.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/LoginFilter</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <servlet-name>MBlogServlet</servlet-name>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <servlet-name>MTagServlet</servlet-name>
  </filter-mapping>
